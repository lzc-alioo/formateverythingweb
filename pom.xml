<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.jd.o2o</groupId>
        <artifactId>o2o-project-parent</artifactId>
        <version>1.0.0-SNAPSHOT</version>
    </parent>

    <groupId>com.lzc</groupId>
    <artifactId>format</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <name>format</name>
    <packaging>pom</packaging>

    <modules>
        <module>format-dao</module>
        <module>format-domain</module>
        <module>format-service</module>
        <module>format-web</module>
    </modules>

    <properties>
        <build.file.encoding>UTF-8</build.file.encoding>
        <log4j2.version>2.3</log4j2.version>
        <disruptor.version>3.3.2</disruptor.version>
        <josl-privilege.version>1.0.2-SNAPSHOT</josl-privilege.version>
        <o2o-commons.version>1.1-SNAPSHOT</o2o-commons.version>
        <tbschedule.version>3.2.14-SNAPSHOT</tbschedule.version>
        <xstream.version>1.4.5</xstream.version>
        <jsf.version>1.2.0-SNAPSHOT</jsf.version>
        <ump.jannotation.version>2.1.0</ump.jannotation.version>
        <ump.profiler.version>3.2.0</ump.profiler.version>
        <jd.jmq.version>1.2.27</jd.jmq.version>
        <deploy-configurer.version>1.2</deploy-configurer.version>
    </properties>

    <distributionManagement>
        <repository>
            <id>releases</id>
            <url>http://artifactory.jd.com/libs-releases</url>
        </repository>
        <snapshotRepository>
            <id>snapshots</id>
            <url>http://artifactory.jd.com/libs-snapshots</url>
        </snapshotRepository>
    </distributionManagement>

    <dependencyManagement>
        <dependencies>
            <!--self dependency -->
            <dependency>
                <groupId>com.lzc</groupId>
                <artifactId>format-domain</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.lzc</groupId>
                <artifactId>format-dao</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>org.apache.logging.log4j</groupId>
                <artifactId>log4j-api</artifactId>
                <version>${log4j2.version}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.logging.log4j</groupId>
                <artifactId>log4j-core</artifactId>
                <version>${log4j2.version}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.logging.log4j</groupId>
                <artifactId>log4j-slf4j-impl</artifactId>
                <version>${log4j2.version}</version>
            </dependency>
            <dependency>
                <groupId>com.lmax</groupId>
                <artifactId>disruptor</artifactId>
                <version>${disruptor.version}</version>
            </dependency>
            <!--o2o.commons -->
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-utils</artifactId>
                <version>1.2-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-monitor</artifactId>
                <version>${o2o-commons.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-cache</artifactId>
                <version>${o2o-commons.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-springmvc</artifactId>
                <version>${o2o-commons.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-dao</artifactId>
                <version>${o2o-commons.version}</version>
            </dependency>
            <dependency>
                <groupId>com.jd.o2o</groupId>
                <artifactId>o2o-commons-domain</artifactId>
                <version>${o2o-commons.version}</version>
            </dependency>
