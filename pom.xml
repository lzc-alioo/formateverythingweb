            <!--</dependency>-->
            <!--json,xml -->
            <dependency>
                <groupId>jackson-all</groupId>
                <artifactId>jackson-all</artifactId>
                <version>${jackson.version}</version>
            </dependency>
            <!-- ump lib begin -->
            <!--<dependency>-->
                <!--<groupId>com.jd.ump</groupId>-->
                <!--<artifactId>jannotation</artifactId>-->
